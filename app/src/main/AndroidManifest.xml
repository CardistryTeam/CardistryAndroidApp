<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nikolay.shuffledex" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".survival.SurvivalModeActivity"
            android:label="@string/title_activity_survival_mode" >
        </activity>
        <activity
            android:name=".versus.VersusModeActivity"
            android:label="@string/title_activity_versus_mode" >
        </activity>
        <activity
            android:name=".practice.PracticeModeActivity"
            android:label="@string/title_activity_practice_mode" >
        </activity>
        <activity
            android:name=".practice.efficiency.EfficiencyPracticeAcitivty"
            android:label="@string/title_activity_efficiency_practice_acitivty" >
        </activity>
        <activity
            android:name=".practice.efficiency.EfficiencyPracticePlayActivity"
            android:label="@string/title_activity_efficiency_practice_play" >
        </activity>
        <activity
            android:name=".practice.efficiency.EfficiencyPracticeFinishActivity"
            android:label="@string/title_activity_efficiency_practice_finish" >
        </activity>
        <activity
            android:name=".practice.speed.SpeedPracticeActivity"
            android:label="@string/title_activity_speed_practice" >
        </activity>
    </application>

</manifest>
